{
    "name": "ermradulsharma/omnilocate",
    "description": "OmniLocate: Advanced user location detection for Laravel with persistence, caching, and distance utilities.",
    "authors": [
        {
            "name": "Mradul Sharma",
            "email": "skywalkerlknw@gmail.com"
        }
    ],
    "license": "MIT",
    "homepage": "https://github.com/ermradulsharma/omniLocate",
    "support": {
        "source": "https://github.com/ermradulsharma/omniLocate",
        "wiki": "https://github.com/ermradulsharma/omniLocate/wiki"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/ermradulsharma"
        }
    ],
    "keywords": [
        "laravel",
        "php",
        "location",
        "omnilocate",
        "geoIP",
        "geo-location",
        "persistence"
    ],
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "ext-curl": "*",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "geoip2/geoip2": "^2.0|^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "autoload": {
        "psr-4": {
            "Ermradulsharma\\OmniLocate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ermradulsharma\\OmniLocate\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Ermradulsharma\\OmniLocate\\LocationServiceProvider"
            ],
            "aliases": {
                "Location": "Ermradulsharma\\OmniLocate\\Facades\\Location"
            }
        }
    }
}